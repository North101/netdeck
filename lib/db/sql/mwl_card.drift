import '../types.dart';

CREATE TABLE mwl_card(
  mwl_code TEXT NOT NULL,
  card_code TEXT NOT NULL,
  is_restricted BOOL NOT NULL,
  global_penalty INT,
  universal_faction_cost INT,
  deck_limit INT,
  points INT,

  PRIMARY KEY (mwl_code, card_code)
);

listMwlCard($where = TRUE):
SELECT *
FROM mwl_card

WHERE $where;