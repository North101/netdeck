CREATE TABLE mwl_card_title(
  mwl_code TEXT NOT NULL,
  card_title TEXT NOT NULL,
  is_restricted BOOL NOT NULL,
  global_penalty INT,
  universal_faction_cost INT,
  deck_limit INT,
  points INT,

  PRIMARY KEY (mwl_code, card_title)
);

listMwlCardTitle($where = TRUE):
SELECT *
FROM mwl_card_title

WHERE $where;